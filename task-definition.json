{
  "family": "syncplay",
  "containerDefinitions": [{
    "image": "godbone/syncplay-server",
    "name": "app",
    "memory": 50,
    "portMappings": [{
      "hostPort": 8999,
      "containerPort": 8999
    }],
    "environment": [{
      "name": "_ECS_R53_DNS", "value": "syncplay.mydomain.tld"
    }, {
      "name": "SALT", "value": ""
    }]
  }]
}
